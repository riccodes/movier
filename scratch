type Movie @model @auth(rules: [{ allow: public }]) {
  id: ID!
  adult: Boolean
  video: Boolean
  backdrop_path: String
  original_title: String
  original_language: String
  overview: String
  popularity: Float
  vote_average: Float
  vote_count: Int
  poster_path: String
  release_date: String
  title: String
  genre_ids: [Int]
}
